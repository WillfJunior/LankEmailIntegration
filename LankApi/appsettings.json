{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=${DB_SERVER};Port=${DB_PORT};Uid=${DB_USERNAME};Pwd=${DB_PASSWORD};Database=${DB_NAME}"
  },
  "Email": {
    "Remetente": "${EMAIL_REMETENTE}",
    "Destinatario": "${EMAIL_DESTINATARIO}",
    "Copia": "${EMAIL_COPIA}",
    "Smtp": "${EMAIL_SMTP}",
    "Porta": "${EMAIL_PORTA}"
}
}
